	keywords = {Asymmetric,Bistable,Composites,Laminates,Modelling,Sensitivity},
	pages = {276--286},
	publisher = {Elsevier Ltd},
	title = {{Sensitivity of bistable laminates to uncertainties in material properties, geometry and environmental conditions}},

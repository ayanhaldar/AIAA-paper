	journal = {Journal of Intelligent Material Systems and Structures},
	keywords = {active shape control,compliant mechanism,composite,morphing,multistable},
	number = {2},
	pages = {157--169},
	title = {{A Concept for Airfoil-like Active Bistable Twisting Structures}},
	volume = {19},
	year = {2007}
}


	keywords = {Anisotropy,Buckling,Compression,Laminated composites,Nondimensional parameters,Plates,Postbuckling,Unsymmetrical configurations},
	number = {22-23},
	pages = {6978--6997},
	title = {{Postbuckling of long unsymmetrically laminated composite plates under axial compression}},
	volume = {43},
	year = {2006}
}
